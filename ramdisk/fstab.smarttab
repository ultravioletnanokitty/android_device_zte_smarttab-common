# Android fstab file.
#<src> <mnt_point> <type> <mnt_flags> <fs_mgr_flags>

/dev/block/mmcblk0p1 /firmware vfat ro,shortname=lower wait
/dev/block/mmcblk0p12 /system ext4 ro wait
/dev/block/mmcblk0p13 /persist ext4 discard,noatime,nosuid,nodev,data=ordered,noauto_da_alloc wait
/dev/block/mmcblk0p14 /cache ext4 discard,noatime,nosuid,nodev,data=ordered,noauto_da_alloc wait
/dev/block/mmcblk0p21 /data ext4 discard,noatime,nosuid,nodev,data=ordered,noauto_da_alloc wait
