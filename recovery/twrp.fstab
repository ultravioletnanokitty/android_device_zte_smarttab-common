# mount point  fstype    device      options (length for cryptfs encryption footer)
/boot		emmc	/dev/block/mmcblk0p8
/recovery	emmc	/dev/block/mmcblk0p18
/system		ext4	/dev/block/mmcblk0p12	flags=backup=1;canbewiped;wipeingui
/data		ext4	/dev/block/mmcblk0p21	flags=backup=1;canbewiped;wipeingui;wipeduringfactoryreset
/cache		ext4	/dev/block/mmcblk0p14	flags=backup=1;canbewiped;wipeingui;wipeduringfactoryreset
/persist	ext4	/dev/block/mmcblk0p13	flags=backup=1;display="persist"
/sdcard		vfat	/dev/block/mmcblk1p1	flags=display="Micro SDcard";storage;wipeingui;removable
